#include<stdio.h>
#include<conio.h>
void main()
{
    char a[100],b[100];
    int i,c=0,j,l;
    scanf("%s",a);
    scanf("%s",b);
    scanf("%d",&l);
    for(i=0;a[i]!='\0';i++)
    {
        for(j=0;b[j]!='\0';j++)
        {
            if(a[i]==b[j])
            {
                c--;
            }
            else
            {
                c++;
            }
        }
    }
if(c==l)
{
    printf("yes");
    
}
else
{
    printf("no");
}
getch();
}
    
